
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">

@Html.Action("PartialHead", "AdminLayout", "Admin")

<body>
    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->
    <div id="main-wrapper">
        @Html.Action("PartialNavbar", "AdminLayout", "Admin")
        
        @Html.Action("PartialSidebar", "AdminLayout", "Admin")
        <div class="content-body">
            <div class="container-fluid">
                @RenderBody()
            </div>
        </div>
    </div>
    @Html.Action("PartialScript", "AdminLayout", "Admin")
</body>

</html>